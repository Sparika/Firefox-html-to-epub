<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>kevin.corre@etu.univ-orleans.fr</em:id>
    <em:name>Save as EPUB</em:name>
    <em:description>Save an HTML page to an EPUB file</em:description>
    <em:version>0.1b</em:version>
    <em:creator>Mozilla/Universite d'Orleans</em:creator>
    <em:homepageURL>http://sparika.github.com/Firefox-html-to-epub/</em:homepageURL>
    <em:type>2</em:type>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>10.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
